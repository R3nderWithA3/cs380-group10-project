<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdminController">
   <children>
      <AnchorPane prefHeight="506.0" prefWidth="407.0">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="28.0" stroke="BLACK" strokeType="INSIDE" width="411.0" />
            <Label layoutX="123.0" layoutY="116.0" text="Registered Accounts">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <TextField fx:id="userID" layoutX="79.0" layoutY="453.0" promptText="Enter User ID" />
            <ImageView fitHeight="88.0" fitWidth="200.0" layoutX="150.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@subscriptLogo.png" />
               </image>
            </ImageView>
            <Button fx:id="toLogin" layoutX="17.0" layoutY="39.0" mnemonicParsing="false" onAction="#closeAdmin" text="Go Back" />
            <Button fx:id="enterUser" layoutX="228.0" layoutY="453.0" mnemonicParsing="false" onAction="#useAcct" prefHeight="25.0" prefWidth="101.0" text="Enter User View" />
            <ListView fx:id="acctList" layoutX="75.0" layoutY="153.0" prefHeight="281.0" prefWidth="263.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
